issn = {0002-9173},
journal = {American journal of clinical pathology},
keywords = {Adenoma,Adenoma: pathology,Adult,Aged,Aged, 80 and over,Biopsy,Colonic Neoplasms,Colonic Neoplasms: pathology,Colonic Polyps,Colonic Polyps: pathology,Colonoscopy,Diagnosis, Differential,Female,Histocytochemistry,Humans,Male,Microtomy,Middle Aged,Paraffin Embedding,Retrospective Studies},
month = mar,
number = {3},
pages = {409--14},
pmid = {17276941},
